<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Slider?><?import javafx.scene.image.ImageView?><?import javafx.scene.layout.*?><?import javafx.scene.media.MediaView?>
<StackPane fx:id="root"
    AnchorPane.bottomAnchor="0.0"
    AnchorPane.leftAnchor="0.0"
    AnchorPane.rightAnchor="0.0"
    AnchorPane.topAnchor="0.0"
    xmlns="http://javafx.com/javafx/10.0.2-internal"
    xmlns:fx="http://javafx.com/fxml/1">

    <MediaView fx:id="playerView" />

    <VBox alignment="BOTTOM_CENTER"
        StackPane.alignment="BOTTOM_CENTER">
        <ImageView fx:id="playback"
            fitHeight="48.0"
            fitWidth="48.0" />

        <Slider fx:id="progress" />
    </VBox>
</StackPane>
